{
    "name": "wndr-dashboard",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "api",
        "web"
    ],
    "scripts": {
        "install:all": "npm install",
        "dev": "cd api && npm run dev & cd web && npm run dev",
        "dev:api": "cd api && npm run dev",
        "dev:web": "cd web && npm run dev",
        "build": "npm run build --workspace=api && npm run build --workspace=web",
        "build:api": "npm run build --workspace=api",
        "build:web": "npm run build --workspace=web",
        "start": "npm run start --workspace=api & npm run start --workspace=web",
        "start:api": "npm run start --workspace=api",
        "start:web": "npm run start --workspace=web",
        "prisma": "npm run prisma --workspace=api",
        "seed": "npm run seed --workspace=api",
        "seed:report": "npm run seed:report --workspace=api"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.624.0",
        "@aws-sdk/s3-request-presigner": "^3.873.0",
        "@prisma/client": "^5.15.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "multer": "^1.4.5-lts.1",
        "next": "^14.2.33",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "recharts": "^2.12.7",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.19.11",
        "@types/react": "^18.2.79",
        "@types/react-dom": "^18.2.25",
        "prisma": "^5.15.0",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsx": "^4.16.2",
        "typescript": "^5.4.5"
    }
}
